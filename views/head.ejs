<body>
    <header class="header">
        <div class="logo">
            <a class="logoText" href="/" id="dark">Atobiloye Usama Adedayo</a>
            <div class="ham_holder">
                <i class="fas fa-bars"></i>
                <p class="sendBtn">Send</p>
            </div>
        </div>
        <div class="navItems">
            <a class="NIText" href="/about" id="dark">About</a>
            <a class="NIText" href="/project" id="dark">Projects</a>
            <a class="LDtoggle"><i class='bx bxs-sun'></i></a>


        </div>
        <div class="menuOverlay">
            <i class='bx bx-sun'></i>
            <a class="mobileNav" href="/">Home</a>
            <a class="mobileNav" href="/about">About</a>
            <a class="mobileNav" href="/project">Projects</a>

        </div>
        <div class="contactModal">
            <div class="cmHeader">
                <p>Get In Touch</p>
                <h3>Schedule an Appointment</h3>
            </div>
            <div class="textInputs">
                <span id='Name'>Name</span>
                <div class="fields">
                    <input class="NameField" type="text" placeholder="Name" required>
                </div>
                <span id='Email'>Email</span>
                <div class="fields">
                    <input class="EmailField" type="email" placeholder="Email" required>
                </div>
                <span id='Message'>Message</span>
                <div class="msgField">
                    <textarea class="MsgField" placeholder="Message" required></textarea>
                </div>
            </div>

            <div class="closeBtn">
                <a class="modalClose"><i class='bx bx-x bx-sm'></i></a>
            </div>
        </div>
    </header>
